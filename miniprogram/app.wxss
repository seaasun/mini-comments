@import './weui-miniprogram/weui-wxss/dist/style/weui.wxss'
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.wxml-box {
  background-color: #EFEFF4;
  width: 100%;
  height: 100%;
  min-height: 100vh;
  padding: 0;
}

.text-clip {
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.text-clip-2 {
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}